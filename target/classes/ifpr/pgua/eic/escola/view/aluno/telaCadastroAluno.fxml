<?xml version="1.0" encoding="UTF-8"?>


<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.geometry.Insets?>


<VBox xmlns="http://javafx.com/javafx" 
      xmlns:fx="http://javafx.com/fxml" 
      fx:controller="ifpr.pgua.eic.escola.controllers.aluno.TelaCadastroAluno"
      >
  
    <VBox styleClass="tela-cadastro">
        <Label text="Nome: " />
        <TextField fx:id="campoNome"/>
        <Label text="Cpf: " />
        <TextField fx:id="campoCpf" />
        <Label text="Email: " />
        <TextField fx:id="campoEmail" />
        <Label text="Telefone: " />
        <VBox spacing="30">
        <TextField fx:id="campoTelefone" />
            <HBox spacing="10">
                <Button styleClass="btn" text="Cadastrar" onAction="#cadastrar"/>
                <Button styleClass="btn" text="Limpar" onAction="#clear" />
            </HBox>
        </VBox>
    </VBox>
</VBox>