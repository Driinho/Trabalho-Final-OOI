<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.geometry.Insets?>


<BorderPane xmlns="http://javafx.com/javafx" 
      xmlns:fx="http://javafx.com/fxml" 
      prefHeight="600" prefWidth="800"
      fx:controller="ifpr.pgua.eic.escola.controllers.aluno.TelaPrincipalAluno"
      styleClass="telaPrincipal"
      >

    <left>
        <VBox alignment="center" spacing="10">
            <Button styleClass="btn" text="Cadastro" onAction="#mudarTelaCadastro"/>
            <Button styleClass="btn" text="Lista" onAction="#mudarTelaLista"/>
            <Button styleClass="btn" text="Matricula" onAction="#mudarTelaMatricula"/>
            <Button styleClass="btn" text="Voltar" onAction="#mudarTelaInicio"/>
        </VBox>
    </left>

    <top>
        <HBox alignment="center">
            <Label styleClass="titulo" text="Aluno" />
        </HBox>
    </top>

</BorderPane>