<?xml version="1.0" encoding="UTF-8"?>


<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.geometry.Insets?>


<VBox xmlns="http://javafx.com/javafx" 
      xmlns:fx="http://javafx.com/fxml" 
      fx:controller="ifpr.pgua.eic.escola.controllers.TelaCadastroAluno"
      >
  
    <padding>
        <Insets topRightBottomLeft="20.0"/>
    </padding>

    <VBox>
        <Label text="Nome: " />
        <TextField fx:id="campoNome"/>
        <Label text="Cpf: " />
        <TextField fx:id="campoCpf" />
        <Label text="Email: " />
        <TextField fx:id="campoEmail" />
        <Label text="Telefone: " />
        <TextField fx:id="campoTelefone" />
        <Label text="Data de Matricula: " />
        <DatePicker fx:id="campoDataMatricula" />
        <HBox spacing="10">
            <Button text="Cadastrar" onAction="#cadastrar"/>
            <Button text="Limpar" onAction="#clear" />
        </HBox>
    </VBox>
</VBox>